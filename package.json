{
	"dependencies": {
		"@t3-oss/env-nextjs": "^0.9.2",
		"next": "14.2.13",
		"nextjs-toploader": "^1.6.12",
		"react": "^18",
		"react-dom": "^18",
		"superstruct": "^2.0.2",
		"zustand": "^4.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.2",
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.2",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.0.1",
		"bun-types": "latest",
		"bundlewatch": "^0.4.0",
		"commitlint": "^19.4.0",
		"lefthook": "^1.7.12",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"turbo": "^2.0.9",
		"typescript": "^5.5.4",
		"typescript-coverage-report": "^1.0.0"
	},
	"name": "next-starter",
	"packageManager": "bun@1.0.27",
	"scripts": {
		"build": "next build",
		"bundlesize:check": "bunx bundlewatch --config bundlewatch.config.json",
		"clear": "rm -rf .next .turbo node_modules",
		"clear:soft": "rm -rf .next .turbo",
		"commitlint:init": "chmod +x .lefthook/commit-msg/commitlint.sh",
		"dev": "next dev --turbo",
		"lefthook:init": "bunx lefthook install",
		"lint": "bunx @biomejs/biome lint ./src",
		"lint:check": "bunx @biomejs/biome check ./src",
		"lint:fix": "bunx @biomejs/biome lint --write ./src",
		"lint:format": "bunx @biomejs/biome format ./src",
		"localtunnel": "bunx localtunnel --port 3000 & node -e \"require('open')('https://loca.lt/mytunnelpassword')\"",
		"preinstall": "bunx only-allow bun",
		"setup": "bunx lefthook install && chmod +x .lefthook/commit-msg/commitlint.sh && bun install",
		"start": "next start",
		"type:check": "tsc --noEmit",
		"type:coverage": "typescript-coverage-report",
		"update:check": "bun x npm-check-updates"
	},
	"type": "module",
	"version": "0.0.1"
}
